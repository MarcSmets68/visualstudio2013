<Window x:Class="Telefoon.TelefoonWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="TelefoonWindow" Height="350" Width="525" Loaded="Window_Loaded">
    <StackPanel>
        <StackPanel Width="300" HorizontalAlignment="Left">
            <ComboBox Name="ComboBoxSelectie" Height="20" Margin="15,15,0,0"></ComboBox>
            <ListBox Name="ListBoxleden" Height="200" Margin="15,30,0,30"></ListBox>
        </StackPanel>
    </StackPanel>
    
</Window>
